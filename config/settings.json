                "indent": "spaces",
                "indent-after": [":", "=", "(", "{", "["],
                "line-comment": "//",
                "block-comment": ["/*", "*/"],
                "completion-word-chars": [["a", "z"], ["A", "Z"], ["0", "9"], "-"],
                "ripgrep.extra-args": ["--type-add", "wit:*.wit"],
                "+search-regexes": {
                    "goto-definition": "((package|world|interface|record|resource|flags|enum|variant) [[0]]\\b)|(\\b[[0]]\\b: (static|func))",
                    "symbols": "((package|world|interface|record|resource|flags|enum|variant) ([a-zA-Z0-9-]+)\\b)|(\\b([a-zA-Z0-9-]+)\\b: (static|func))",
                    "workspace-symbols-by-kind": {
                        "Class": "((package|world|interface|record|resource|flags|enum|variant) ([a-zA-Z0-9-]+)\\b)",
                        "Function": "(\\b([a-zA-Z0-9-]+)\\b: (static|func))",
                    }
                }
            }
        },
    },

    "editor.shells.wsl": {
        "command": "wsl",
        "args": ["--"],
        "eval": false,
    },
    "editor.shells.powershell": {
        "command": "powershell",
        "args": ["-Command"],
        "eval": false,
    },
    "editor.shells.bash": {
        "command": "bash",
        "args": ["-c"],
        "eval": false,
    },
    "editor.shells.sh": {
        "command": "sh",
        "args": ["-c"],
        "eval": false,
    },
    "editor.shells.zsh": {
        "command": "zsh",
        "args": ["-c"],
        "eval": false,
    },

    "editor.open-session.tmux": {
        "env": "TMUX",
        "command": "tmux",
        "args": ["split-window", ["exe"] , ["args"], "--terminal"],
    },
    "editor.open-session.zellij": {
        "env": "ZELLIJ",
        "command": "zellij",
        "args": ["run", "--", ["exe"] , ["args"], "--terminal"],
    },
    "editor.open-session.wezterm": {
        "env": "WEZTERM_EXECUTABLE",
        "command": "wezterm",
        "args": ["cli", "split-pane", "--right", "--", ["exe"] , ["args"], "--terminal"],
    },

    "ui.scroll-speed": 100,

    "ui.layout.default": "nev",
    "ui.layout.vim": {
        "slots.default": "**.*",
        "slots.new-tab": "+",
        "slots.scratch-terminal": "**.*<>",
        "slots.build-run-terminal": "**.*<>",
        "slots.debugger": "**.*<>",
        "kind": "tab",
    },
    "ui.layout.vscode": {
        "slots.default": "center.*.+",
        "slots.scratch-terminal": "bottom.+",
        "slots.build-run-terminal": "left.+",
        "slots.debugger": "left.+",
        "slots.new-tab": "**.+",
        "kind": "center",
        "centerTemplate": {
            "kind": "alternating",
            "childTemplate": {
                "kind": "tab"
            }
        },
        "bottomTemplate": {
            "kind": "tab",
        },
        "leftTemplate": {
            "kind": "tab",
        },
        "rightTemplate": {
            "kind": "tab",
        },
    },
    "ui.layout.nev": {
        "slots.default": "*.center.**.*",
        "slots.new-tab": "+.center.*",
        "slots.scratch-terminal": "*.center.**.*<>",
        "slots.build-run-terminal": "*.center.**.*<>",
        "slots.debugger": "*.left.+",
        "slots.debugger-threads": "*.right.+",
        "slots.debugger-stacktrace": "*.right.+",
        "slots.debugger-toolbar": "*.right.+",
        "slots.debugger-variables": "*.left",
        "slots.debugger-output": "*.bottom",
        "kind": "tab",
        "childTemplate": {
            "kind": "center",
            "rightTemplate": {
                "kind": "vertical",
                "split-ratios": [0.1, 0.25],
                "temporary": false,
            }
        }
    },

    "extra-settings": ["app://config/settings-vim.json"],

    "alias": {
        "explore-root": ["explore-files \"\""],
        "explore-workspace": ["explore-files \"ws0://\""],
        "explore-user-config": ["explore-files \"home://.nev\""],
        "explore-app-config": ["explore-files \"app://config\""],
        "explore-workspace-config": ["explore-files \"ws0://.nev\""],
        "explore-help": ["explore-files \"app://docs\""],
    },

    "plugin.vim.permissions": {
        "commands": {
            "allowAll": true,
        },
    },

}