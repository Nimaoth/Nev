{
    "<set-LEADER>": ["<SPACE>"],
    "<set-WINDOW>": ["<C-w>"],

    "command-line-low": {
        "<ESCAPE>": "vim-exit-command-line",
        "<ENTER>": "execute-command-line",
        "<UP>": "select-previous-command-in-history",
        "<DOWN>": "select-next-command-in-history",
