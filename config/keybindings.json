{
    "editor": {
        "<SPACE>sr": "open-recent-session",
        "!": {
            "command": "run-shell-command",
            "args": [{"shell": "default", "showInCommandLine": false, "appendAndShowInFile": true}]
        }
    },
    "popup.selector.settings": {
        "<C-h>": "prev-store",
        "<C-f>": "next-store",
        "<C-g>": "toggle-merged",
        "<C-k>": "show-preview-editor-settings",
        "<C-c>": "delete-setting",
        "<C-r>": "reload-store"
    },
    "popup.selector.file-explorer": {
        "<C-g>n": "create-new-session",
        "<C-g>o": "open-session",
    }
}