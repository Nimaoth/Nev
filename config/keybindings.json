        "<?-count>d<move>": ["vim-delete-move <move> <#count>"],
        "<?-count>c<move>": ["vim-change-move <move> <#count>"],
        "<?-count>y<move>": ["vim-yank-move <move> <#count>"],

        "<?-count>d<text_object>": ["vim-delete-move <text_object> <#count>"],
        "<?-count>c<text_object>": ["vim-change-move <text_object> <#count>"],
        "<?-count>y<text_object>": ["vim-yank-move <text_object> <#count>"],

