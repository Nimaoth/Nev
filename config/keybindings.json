        "<ESCAPE>": "escape",
        "<SPACE>sr": "open-recent-session",
        "<SPACE>to": ["select-terminal"],
        "<SPACE>ts": [
            "run-in-terminal",
            "default",
            "",
            {
                "mode": "insert",
                "closeOnTerminate": true,
                "group": "scratch",
            }
        ],

