        "<A-8>": ["set-active-view-index", "", 7],
        "<A-9>": ["set-active-view-index", "", 8],

        "<C-s>": ["write-file"],
        "<LEADER><LEADER>": ["command-line"],
        "<LEADER>gt": ["choose-theme"],
        "<LEADER>gf": ["choose-file"],
        "<LEADER>go": ["choose-open"],
        "<LEADER>gd": ["choose-open-document"],
        "<LEADER>gl": [".fuzzy-search-lines"],
        "<LEADER>gL": [".fuzzy-search-lines", 0.4, false],
        "<LEADER>gg": ["choose-git-active-files", false],
        "<LEADER>GG": ["choose-git-active-files", true],
        "<LEADER>ge": ["explore-root"],
        "<LEADER>gv": ["explore-files", "", true],
        "<LEADER>gw": ["explore-workspace"],
        // "<LEADER>gW": ["explore-files", "ws0://".normalizePath],
        "<LEADER>gu": ["explore-user-config"],
        "<LEADER>ga": ["explore-app-config"],
        "<LEADER>gh": ["explore-help"],
        "<LEADER>gp": ["explore-current-file-directory"],
        "<LEADER>gs": ["search-global-interactive"],
        "<LEADER>gk": ["browse-keybinds"],
        "<LEADER>gi": ["browse-settings"],
        "<LEADER>gn": ["open-last-view"],
        "<LEADER>fl": ["load-file"],
        "<LEADER>fs": ["write-file"],

        "<LEADER>ac": ["continue-execution"],
        "<LEADER>ar": ["step-over"],
        "<LEADER>at": ["step-in"],
        "<LEADER>an": ["step-out"],
        "<LEADER>ae": ["edit-breakpoints"],
        "<LEADER>am": ["toggle-breakpoints-enabled"],
        "<LEADER>gb": ["show-debugger-view"],
        "<LEADER>al": { "command": ["all", "run-last-configuration", "show-debugger-view"], "description": "Run last configuration" },
        "<LEADER>av": { "command": ["all", "choose-run-configuration", "show-debugger-view"], "description": "Choose run configuration" },
        "<LEADER>ab": { "command": ["toggle-breakpoint"], "description": "Toggle breakpoint" },

        "<LEADER>g": { "description": "Global pickers" },
        "<LEADER>a": { "description": "Debugger" },
        "<LEADER>o": { "description": "Options" },
        "<LEADER>ff": { "description": "Rendering" },
        "<LEADER>w": { "description": "Window" },
        "<LEADER>wf": { "description": "Change font size" },
        "<LEADER>wk": { "description": "Split size ratio" },
        "<LEADER>r": { "description": "Run" },
        "<LEADER>f": { "description": "File" },

